<%= form_for(@article) do |f| %>
	<%= f.label :title %>
	<%= f.text_field :title, placeholder: "请输入标题！" %>
	<%= f.label :description %>
	<%= f.text_area :description %>
	<%= f.label :picurl %>
	<%= f.text_field :picurl, placeholder: "请输入图片路径！" %>
	<%= f.label :url %>
	<%= f.text_field :url, placeholder: "请出入文章路径！" %>
	<%= f.submit %>
<% end %> 
